{
  "config": {
    "allow-plugins": {
      "composer/installers": true
    }
  },
  "extra": {
    "installer-paths": {
      "./app/public/wp-content/plugins/{$name}/": [
        "type:wordpress-plugin"
      ]
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org",
      "only": [
        "wpackagist-plugin/*"
      ]
    }
  ],
  "require-dev": {
    "wpackagist-plugin/debug-bar": "^1.1"
  },
  "require": {
    "wpackagist-plugin/duplicate-post": "^4.5",
    "wpackagist-plugin/favicon-by-realfavicongenerator": "^1.3",
    "wpackagist-plugin/imagify": "2.2.0.1",
    "wpackagist-plugin/imsanity": "^2.8",
    "wpackagist-plugin/svg-support": "^2.5",
    "wpackagist-plugin/wordpress-seo": "^22.2",
    "wpackagist-plugin/wp-sweep": "^1.1"
  }
}
