{
  "config": {
    "allow-plugins": {
      "composer/installers": true
    }
  },
  "extra": {
    "installer-paths": {
      "./app/public/wp-content/plugins/{$name}/": [
        "type:wordpress-plugin"
      ]
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org",
      "only": [
        "wpackagist-plugin/*"
      ]
    }
  ],
  "require-dev": {
    "wpackagist-plugin/debug-bar": "^1.1.6",
    "php-stubs/wordpress-seo-stubs": "^20.5.1"
  },
  "require": {
    "wpackagist-plugin/duplicate-post": "^4.5",
    "wpackagist-plugin/favicon-by-realfavicongenerator": "^1.3.41",
    "wpackagist-plugin/imagify": "^2.2.5",
    "wpackagist-plugin/imsanity": "^2.8.6",
    "wpackagist-plugin/svg-support": "^2.5.14",
    "wpackagist-plugin/wordpress-seo": "^25.0",
    "wpackagist-plugin/wp-sweep": "^1.1.8"
  },
  "type": "project"
}
