{
  "extra": {
    "installer-paths": {
      "./app/public/wp-content/plugins/{$name}/": [
        "type:wordpress-plugin"
      ]
    }
  },
  "require-dev": {
    "wpackagist-plugin/debug-bar": "^1"
  },
  "require": {
    "wpackagist-plugin/duplicate-post": "^4",
    "wpackagist-plugin/favicon-by-realfavicongenerator": "^1",
    "wpackagist-plugin/imagify": "^1",
    "wpackagist-plugin/imsanity": "^2",
    "wpackagist-plugin/svg-support": "^2",
    "wpackagist-plugin/wordpress-seo": "^18",
    "wpackagist-plugin/wp-sweep": "^1"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org",
      "only": [
        "wpackagist-plugin/*"
      ]
    }
  ],
  "config": {
    "allow-plugins": {
      "composer/installers": true
    }
  }
}
